{
  "name": "dex-order-execution-engine",
  "version": "1.0.0",
  "description": "Mock DEX Order Execution Engine with BullMQ, WebSocket, and dual DEX routing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "tsx src/workers/executor.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "dex",
    "order-execution",
    "bullmq",
    "websocket",
    "fastify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/websocket": "^10.0.1",
    "axios": "^1.13.2",
    "bullmq": "^5.15.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pg": "^8.13.1",
    "pino-pretty": "^10.1.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
